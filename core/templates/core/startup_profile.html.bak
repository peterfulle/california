{% extends 'base.html' %}

{% block title %}{{ startup.company_name }} - Perfil{% endblock %}

{% block content %}
<div class="min-h-screen bg-gray-50">
    <!-- Header de la Startup -->
    <div class="bg-white border-b border-gray-200">
        <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
            <div class="py-6">
                <div class="flex items-start space-x-6">
                    <!-- Logo -->
                    <div class="flex-shrink-0">
                        {% if startup.logo %}
                            <img class="h-20 w-20 rounded-lg object-cover" src="{{ startup.logo.url }}" alt="{{ startup.company_name }}">
                        {% else %}
                            <div class="h-20 w-20 bg-gradient-to-br from-blue-400 to-blue-600 rounded-lg flex items-center justify-center">
                                <span class="text-white text-2xl font-bold">{{ startup.company_name|first }}</span>
                            </div>
                        {% endif %}
                    </div>
                    
                    <!-- Información Principal -->
                    <div class="flex-1 min-w-0">
                        <div class="flex items-center justify-between">
                            <div>
                                <h1 class="text-2xl font-bold text-gray-900">{{ startup.company_name }}</h1>
                                <div class="mt-1 flex items-center space-x-4 text-sm text-gray-500">
                                    <div class="flex items-center">
                                        <span class="font-medium text-green-600">Growth Score</span>
                                        <span class="ml-1 font-bold text-gray-900">{{ growth_score }}</span>
                                    </div>
                                    <div class="flex items-center">
                                        <span class="font-medium text-gray-600">CB Rank</span>
                                        <span class="ml-1 font-bold text-gray-900">{{ cb_rank }}</span>
                                        <span class="ml-1 text-sm text-gray-500">({{ ranking_percentile }}th percentile)</span>
                                    </div>
                                    <div class="flex items-center">
                                        <span class="font-medium text-orange-600">Heat Score</span>
                                        <span class="ml-1 font-bold text-gray-900">{{ heat_score }}</span>
                                    </div>
                                </div>
                            </div>
                            
                            <!-- Acciones -->
                            <div class="flex space-x-3">
                                <button class="inline-flex items-center px-4 py-2 border border-gray-300 rounded-md text-sm font-medium text-gray-700 bg-white hover:bg-gray-50">
                                    <svg class="w-4 h-4 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4.318 6.318a4.5 4.5 0 000 6.364L12 20.364l7.682-7.682a4.5 4.5 0 00-6.364-6.364L12 7.636l-1.318-1.318a4.5 4.5 0 00-6.364 0z"></path>
                                    </svg>
                                    Seguir
                                </button>
                                <button class="inline-flex items-center px-4 py-2 bg-blue-600 border border-transparent rounded-md text-sm font-medium text-white hover:bg-blue-700">
                                    <svg class="w-4 h-4 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 12h.01M12 12h.01M16 12h.01M21 12c0 4.418-3.582 8-8 8a8.955 8.955 0 01-2.172-.267l-2.628 1.543A1 1 0 017 20.414l1.543-2.628A8.955 8.955 0 014 17c0-4.418 3.582-8 8-8s8 3.582 8 8z"></path>
                                    </svg>
                                    Contactar
                                </button>
                            </div>
                        </div>
                        
                        <!-- Descripción Breve -->
                        <p class="mt-3 text-gray-900 text-base">{{ startup.tagline|default:startup.description|truncatewords:25 }}</p>
                        
                        <!-- Detalles Básicos -->
                        <div class="mt-4 flex flex-wrap items-center text-sm text-gray-600 space-x-6">
                            {% if startup.founded_date %}
                            <div class="flex items-center">
                                <svg class="w-4 h-4 mr-1 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 7V3m8 4V3m-9 8h10M5 21h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v12a2 2 0 002 2z"></path>
                                </svg>
                                Fundada {{ startup.founded_date|date:"M j, Y" }}
                            </div>
                            {% endif %}
                            
                            <div class="flex items-center">
                                <svg class="w-4 h-4 mr-1 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 2l3.09 6.26L22 9.27l-5 4.87 1.18 6.88L12 17.77l-6.18 3.25L7 14.14 2 9.27l6.91-1.01L12 2z"></path>
                                </svg>
                                Privada
                            </div>
                            
                            {% if startup.stage %}
                            <div class="flex items-center">
                                <svg class="w-4 h-4 mr-1 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 7h8m0 0v8m0-8l-8 8-4-4-6 6"></path>
                                </svg>
                                {{ startup.get_stage_display }}
                            </div>
                            {% endif %}
                            
                            <div class="flex items-center">
                                <svg class="w-4 h-4 mr-1 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17 20h5v-2a3 3 0 00-5.356-1.857M17 20H7m10 0v-2c0-.656-.126-1.283-.356-1.857M7 20H2v-2a3 3 0 015.356-1.857M7 20v-2c0-.656.126-1.283.356-1.857m0 0a5.002 5.002 0 019.288 0M15 7a3 3 0 11-6 0 3 3 0 016 0zm6 3a2 2 0 11-4 0 2 2 0 014 0zM7 10a2 2 0 11-4 0 2 2 0 014 0z"></path>
                                </svg>
                                {{ startup.employees_count|default:"1-10" }} empleados
                            </div>
                            
                            {% if startup.website %}
                            <div class="flex items-center">
                                <svg class="w-4 h-4 mr-1 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13.828 10.172a4 4 0 00-5.656 0l-4 4a4 4 0 105.656 5.656l1.102-1.101m-.758-4.899a4 4 0 005.656 0l4-4a4 4 0 00-5.656-5.656l-1.1 1.1"></path>
                                </svg>
                                <a href="{{ startup.website }}" target="_blank" class="text-blue-600 hover:text-blue-800">{{ startup.website|slice:"8:" }}</a>
                            </div>
                            {% endif %}
                        </div>
                        
                        <!-- Industrias -->
                        {% if startup.industry %}
                        <div class="mt-4 flex flex-wrap gap-2">
                            <span class="inline-flex items-center px-2.5 py-0.5 rounded-full text-xs font-medium bg-blue-100 text-blue-800">
                                {{ startup.industry.name }}
                            </span>
                            {% if startup.sub_industry %}
                            <span class="inline-flex items-center px-2.5 py-0.5 rounded-full text-xs font-medium bg-green-100 text-green-800">
                                {{ startup.sub_industry }}
                            </span>
                            {% endif %}
                        </div>
                        {% endif %}
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- Navegación de Tabs -->
    <div class="bg-white border-b border-gray-200">
        <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
            <nav class="-mb-px flex space-x-8" id="tab-navigation">
                <button class="tab-button active border-blue-500 text-blue-600 whitespace-nowrap py-4 px-1 border-b-2 font-medium text-sm" data-tab="overview">
                    Overview
                </button>
                <button class="tab-button border-transparent text-gray-500 hover:text-gray-700 hover:border-gray-300 whitespace-nowrap py-4 px-1 border-b-2 font-medium text-sm" data-tab="predictions">
                    Predictions & Insights
                </button>
                
                <!-- Tabs con información privada -->
                <div class="relative inline-block">
                    <button class="tab-button border-transparent text-gray-500 hover:text-gray-700 hover:border-gray-300 whitespace-nowrap py-4 px-1 border-b-2 font-medium text-sm flex items-center" data-tab="financials">
                        Financials
                        {% if user.is_authenticated and user.profile.user_type == 'investor' %}
                            <svg class="w-4 h-4 ml-1 text-orange-500" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 15v2m0 0v3m0-3h3m-3 0h-3m9-6V9a2 2 0 00-2-2H5a2 2 0 00-2 2v7a2 2 0 002 2h14a2 2 0 002-2z"></path>
                            </svg>
                        {% endif %}
                    </button>
                </div>
                
                <div class="relative inline-block">
                    <button class="tab-button border-transparent text-gray-500 hover:text-gray-700 hover:border-gray-300 whitespace-nowrap py-4 px-1 border-b-2 font-medium text-sm flex items-center" data-tab="people">
                        People
                        {% if user.is_authenticated and user.profile.user_type == 'investor' %}
                            <svg class="w-4 h-4 ml-1 text-orange-500" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 15v2m0 0v3m0-3h3m-3 0h-3m9-6V9a2 2 0 00-2-2H5a2 2 0 00-2 2v7a2 2 0 002 2h14a2 2 0 002-2z"></path>
                            </svg>
                        {% endif %}
                    </button>
                </div>
                
                <div class="relative inline-block">
                    <button class="tab-button border-transparent text-gray-500 hover:text-gray-700 hover:border-gray-300 whitespace-nowrap py-4 px-1 border-b-2 font-medium text-sm flex items-center" data-tab="news">
                        News
                        {% if user.is_authenticated and user.profile.user_type == 'investor' %}
                            <svg class="w-4 h-4 ml-1 text-orange-500" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 15v2m0 0v3m0-3h3m-3 0h-3m9-6V9a2 2 0 00-2-2H5a2 2 0 00-2 2v7a2 2 0 002 2h14a2 2 0 002-2z"></path>
                            </svg>
                        {% endif %}
                    </button>
                </div>
                
                <div class="relative inline-block">
                    <button class="tab-button border-transparent text-gray-500 hover:text-gray-700 hover:border-gray-300 whitespace-nowrap py-4 px-1 border-b-2 font-medium text-sm flex items-center" data-tab="technology">
                        Technology
                        {% if user.is_authenticated and user.profile.user_type == 'investor' %}
                            <svg class="w-4 h-4 ml-1 text-orange-500" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 15v2m0 0v3m0-3h3m-3 0h-3m9-6V9a2 2 0 00-2-2H5a2 2 0 00-2 2v7a2 2 0 002 2h14a2 2 0 002-2z"></path>
                            </svg>
                        {% endif %}
                    </button>
                </div>
            </nav>
        </div>
    </div>

    <!-- Contenido Principal -->
    <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8 py-8">
        <!-- Tab Content: Overview -->
        <div id="tab-overview" class="tab-content active">
            <div class="grid grid-cols-1 lg:grid-cols-3 gap-8">
                <!-- Columna Principal -->
                <div class="lg:col-span-2 space-y-8">
                    <!-- Overview -->
                    <div class="bg-white rounded-lg shadow-sm border border-gray-200 p-6">
                        <h2 class="text-lg font-semibold text-gray-900 mb-4">Overview</h2>
                        
                        <!-- Métricas de Performance con Grade -->
                        <div class="grid grid-cols-3 gap-6 mb-6">
                            <div>
                                <div class="flex items-center justify-between mb-2">
                                    <span class="text-sm font-medium text-gray-600">Growth Score</span>
                                    <div class="px-2 py-1 rounded text-xs font-medium bg-{{ performance_grade.color }}-100 text-{{ performance_grade.color }}-800">
                                        {{ performance_grade.grade }}
                                    </div>
                                </div>
                                <div class="text-3xl font-bold text-gray-900">{{ growth_score }}</div>
                                <div class="text-sm text-green-600">{{ market_position }}</div>
                            </div>
                            
                            <div>
                                <div class="flex items-center justify-between mb-2">
                                    <span class="text-sm font-medium text-gray-600">Heat Score</span>
                                    <button class="text-gray-400 hover:text-gray-600">
                                        <svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 16h-1v-4h-1m1-4h.01M21 12a9 9 0 11-18 0 9 9 0 0118 0z"></path>
                                        </svg>
                                    </button>
                                </div>
                                <div class="text-3xl font-bold text-gray-900">{{ heat_score }}</div>
                                <div class="text-sm text-orange-600">{{ ranking_percentile }}th percentile</div>
                            </div>

                            <div>
                                <div class="flex items-center justify-between mb-2">
                                    <span class="text-sm font-medium text-gray-600">CB Rank</span>
                                    <span class="text-xs text-gray-500">Global</span>
                                </div>
                                <div class="text-3xl font-bold text-gray-900">#{{ cb_rank }}</div>
                                <div class="text-sm text-blue-600">{{ formatted_revenue }}</div>
                            </div>
                        </div>
                        
                        <!-- Performance Metrics -->
                        <div class="mb-6">
                            <h3 class="text-sm font-medium text-gray-900 mb-3">Company Performance Metrics</h3>
                            <div class="bg-gray-50 rounded-lg p-4">
                                <div class="flex items-center justify-between mb-2">
                                    <span class="text-sm text-gray-600">Company Profile Activity</span>
                                    <span class="text-sm font-medium text-orange-600">{{ startup.is_fundraising|yesno:"Fundraising,Stable" }}</span>
                                </div>
                                <div class="text-xs text-gray-500 mb-3">Entre todas las empresas {{ startup.industry.name|default:"SaaS" }}</div>
                                <div class="flex items-center justify-between text-xs text-gray-400">
                                    <span>Baja Actividad</span>
                                    <span>Alta Actividad</span>
                                </div>
                                <div class="w-full bg-gray-200 rounded-full h-2 mt-1">
                                    <div class="bg-orange-500 h-2 rounded-full" style="width: {{ heat_score }}%"></div>
                                </div>
                            </div>
                        </div>

                        <!-- Recent Updates -->
                        <div class="mb-6">
                            <h3 class="text-sm font-medium text-gray-900 mb-3">Recent Updates</h3>
                            <div class="space-y-3">
                                <div class="flex items-start space-x-3">
                                    <div class="flex-shrink-0 w-2 h-2 bg-green-400 rounded-full mt-2"></div>
                                    <div class="flex-1">
                                        <p class="text-sm text-gray-900">Profile updated with latest metrics</p>
                                        <p class="text-xs text-gray-500">{{ startup.updated_at|timesince }} ago</p>
                                    </div>
                                </div>
                                {% if startup.is_fundraising %}
                                <div class="flex items-start space-x-3">
                                    <div class="flex-shrink-0 w-2 h-2 bg-blue-400 rounded-full mt-2"></div>
                                    <div class="flex-1">
                                        <p class="text-sm text-gray-900">Actively fundraising</p>
                                        <p class="text-xs text-gray-500">Seeking {{ formatted_funding }}</p>
                                    </div>
                                </div>
                                {% endif %}
                            </div>
                        </div>
                    </div>
                    
                    <!-- Detalles de la Empresa -->
                    <div class="bg-white rounded-lg shadow-sm border border-gray-200 p-6">
                        <h2 class="text-lg font-semibold text-gray-900 mb-4">Company Details</h2>
                        
                        <div class="grid grid-cols-2 gap-4 text-sm">
                            <div>
                                <dt class="font-medium text-gray-900">Legal Name</dt>
                                <dd class="text-gray-600">{{ startup.company_name }}</dd>
                            </div>
                            
                            <div>
                                <dt class="font-medium text-gray-900">Operating Status</dt>
                                <dd class="text-gray-600">Active</dd>
                            </div>
                            
                            <div>
                                <dt class="font-medium text-gray-900">Company Type</dt>
                                <dd class="text-gray-600">Private</dd>
                            </div>
                            
                            <div>
                                <dt class="font-medium text-gray-900">Stage</dt>
                                <dd class="text-gray-600">{{ startup.get_stage_display }}</dd>
                            </div>

                            {% if startup.founded_date %}
                            <div>
                                <dt class="font-medium text-gray-900">Founded</dt>
                                <dd class="text-gray-600">{{ startup.founded_date|date:"M j, Y" }}</dd>
                            </div>
                            {% endif %}
                            
                            {% if startup.website %}
                            <div>
                                <dt class="font-medium text-gray-900">Website</dt>
                                <dd class="text-gray-600">
                                    <a href="{{ startup.website }}" target="_blank" class="text-blue-600 hover:text-blue-800">{{ startup.website }}</a>
                                </dd>
                            </div>
                            {% endif %}
                        </div>
                    </div>
                    
                    <!-- Acerca de la Empresa -->
                    {% if startup.description %}
                    <div class="bg-white rounded-lg shadow-sm border border-gray-200 p-6">
                        <h2 class="text-lg font-semibold text-gray-900 mb-4">About {{ startup.company_name }}</h2>
                        <div class="prose prose-sm max-w-none text-gray-600">
                            {{ startup.description|linebreaks }}
                        </div>
                    </div>
                    {% endif %}
                </div>
                
                <!-- Sidebar -->
                <div class="space-y-6">
                    <!-- Financiación -->
                    <div class="bg-white rounded-lg shadow-sm border border-gray-200 p-6">
                        <h3 class="text-lg font-semibold text-gray-900 mb-4">Funding Overview</h3>
                        
                        <div class="space-y-4">
                            <div>
                                <dt class="text-sm font-medium text-gray-900">Total Funding</dt>
                                <dd class="text-lg font-bold text-gray-900">{{ formatted_funding }}</dd>
                            </div>
                            
                            {% if startup.seeking_amount %}
                            <div>
                                <dt class="text-sm font-medium text-gray-900">Currently Seeking</dt>
                                <dd class="text-sm text-green-600">${{ startup.seeking_amount|floatformat:0 }}</dd>
                            </div>
                            {% endif %}
                            
                            <div>
                                <dt class="text-sm font-medium text-gray-900">Funding Rounds</dt>
                                <dd class="text-sm text-gray-600">{{ total_funding_rounds }} round{{ total_funding_rounds|pluralize }}</dd>
                            </div>
                        </div>
                    </div>
                    
                    <!-- Métricas Clave -->
                    <div class="bg-white rounded-lg shadow-sm border border-gray-200 p-6">
                        <h3 class="text-lg font-semibold text-gray-900 mb-4">Key Metrics</h3>
                        
                        <div class="space-y-3">
                            {% if startup.monthly_revenue %}
                            <div>
                                <dt class="text-sm font-medium text-gray-900">Monthly Revenue</dt>
                                <dd class="text-sm text-gray-600">{{ formatted_revenue }}</dd>
                            </div>
                            {% endif %}
                            
                            {% if startup.monthly_users %}
                            <div>
                                <dt class="text-sm font-medium text-gray-900">Monthly Users</dt>
                                <dd class="text-sm text-gray-600">{{ startup.monthly_users|floatformat:0 }}</dd>
                            </div>
                            {% endif %}
                            
                            <div>
                                <dt class="text-sm font-medium text-gray-900">Team Size</dt>
                                <dd class="text-sm text-gray-600">{{ startup.employees_count|default:"1-10" }} employees</dd>
                            </div>

                            <div>
                                <dt class="text-sm font-medium text-gray-900">Industry</dt>
                                <dd class="text-sm text-gray-600">{{ startup.industry.name|default:"Technology" }}</dd>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!-- Tab Content: Predictions & Insights -->
        <div id="tab-predictions" class="tab-content hidden">
            <div class="grid grid-cols-1 lg:grid-cols-3 gap-8">
                <!-- Main Content -->
                <div class="lg:col-span-2 space-y-8">
                    <!-- AI-Powered Insights -->
                    <div class="bg-white rounded-lg shadow-sm border border-gray-200 p-6">
                        <div class="flex items-center mb-4">
                            <svg class="w-6 h-6 text-purple-600 mr-2" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9.663 17h4.673M12 3v1m6.364 1.636l-.707.707M21 12h-1M4 12H3m3.343-5.657l-.707-.707m2.828 9.9a5 5 0 117.072 0l-.548.547A3.374 3.374 0 0014 18.469V19a2 2 0 11-4 0v-.531c0-.895-.356-1.754-.988-2.386l-.548-.547z"></path>
                            </svg>
                            <h2 class="text-lg font-semibold text-gray-900">AI-Powered Growth Insights</h2>
                        </div>
                        
                        <!-- Growth Prediction -->
                        <div class="bg-gradient-to-r from-purple-50 to-blue-50 rounded-lg p-4 mb-6">
                            <h3 class="text-sm font-medium text-gray-900 mb-2">6-Month Growth Prediction</h3>
                            <div class="flex items-center space-x-4">
                                <div class="text-2xl font-bold text-green-600">+{{ growth_score|add:15 }}%</div>
                                <div class="text-sm text-gray-600">Expected revenue growth based on current trajectory</div>
                            </div>
                        </div>

                        <!-- Market Insights -->
                        <div class="grid grid-cols-1 md:grid-cols-2 gap-4">
                            <div class="bg-green-50 border border-green-200 rounded-lg p-4">
                                <h4 class="font-medium text-green-800 mb-2">Growth Opportunities</h4>
                                <ul class="text-sm text-green-700 space-y-1">
                                    <li>• {{ startup.industry.name|default:"SaaS" }} market growing 15% YoY</li>
                                    <li>• Strong product-market fit indicators</li>
                                    <li>• Expanding team suggests scaling phase</li>
                                </ul>
                            </div>
                            
                            <div class="bg-yellow-50 border border-yellow-200 rounded-lg p-4">
                                <h4 class="font-medium text-yellow-800 mb-2">Risk Factors</h4>
                                <ul class="text-sm text-yellow-700 space-y-1">
                                    <li>• Competitive landscape intensifying</li>
                                    <li>• Customer acquisition costs trending up</li>
                                    <li>• Regulatory changes in {{ startup.industry.name|default:"tech" }}</li>
                                </ul>
                            </div>
                        </div>
                    </div>

                    <!-- Benchmark Analysis -->
                    <div class="bg-white rounded-lg shadow-sm border border-gray-200 p-6">
                        <h2 class="text-lg font-semibold text-gray-900 mb-4">Benchmark Analysis</h2>
                        
                        <!-- Industry Comparison -->
                        <div class="space-y-4">
                            <div>
                                <div class="flex justify-between text-sm mb-1">
                                    <span>Revenue Growth (vs Industry)</span>
                                    <span class="font-medium">{{ startup.company_name }}: Above Average</span>
                                </div>
                                <div class="w-full bg-gray-200 rounded-full h-2">
                                    <div class="bg-green-500 h-2 rounded-full" style="width: 70%"></div>
                                </div>
                            </div>
                            
                            <div>
                                <div class="flex justify-between text-sm mb-1">
                                    <span>Team Growth Rate</span>
                                    <span class="font-medium">{{ growth_score }}% vs 45% industry avg</span>
                                </div>
